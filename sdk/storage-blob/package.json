{
  "name": "@gllawuk/nestjs-azure-storage-blob",
  "version": "0.0.1",
  "description": "Azure Storage Blob client for NestJS",
  "homepage": "https://github.com/gllawuk/nestjs-azure-sdk/tree/main/sdk/storage-blob",
  "repository": "github:gllawuk/nestjs-azure-sdk",
  "bugs": {
    "url": "https://github.com/gllawuk/nestjs-azure-sdk/issues/"
  },
  "author": "Julian Dawson",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --ext ts --fix lib",
    "format": "prettier --write \"lib/**/*.ts\"",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@azure/storage-blob": "12.10.0",
    "@nestjs/common": "8.4.5",
    "@nestjs/core": "8.4.5",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "typescript": "4.6.4"
  },
  "main": "./dist/index.js",
  "keywords": [
    "nodejs",
    "javascript",
    "typescript",
    "azure",
    "nestjs"
  ]
}
