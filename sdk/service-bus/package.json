{
  "name": "@gllawuk/nestjs-azure-service-bus",
  "version": "0.0.6",
  "description": "NestJS custom transport for Azure Service Bus",
  "homepage": "https://github.com/gllawuk/nestjs-azure-sdk/tree/main/sdk/service-bus",
  "repository": "github:gllawuk/nestjs-azure-sdk",
  "bugs": {
    "url": "https://github.com/gllawuk/nestjs-azure-sdk/issues/"
  },
  "author": "Julian Dawson",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --ext ts --fix lib",
    "format": "prettier --write \"lib/**/*.ts\"",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@azure/service-bus": "7.5.1",
    "@nestjs/common": "8.4.7",
    "@nestjs/core": "8.4.7",
    "@nestjs/microservices": "8.4.7",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "typescript": "4.7.4"
  },
  "main": "./dist/index.js",
  "keywords": [
    "nodejs",
    "javascript",
    "typescript",
    "azure",
    "nestjs"
  ]
}
