    "reflect-metadata": "^0.1.12",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@azure/service-bus": "^7.5.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.12",
    "release-it": "^14.12.5",
    "rxjs": "^7.1.0",
    "typescript": "^4.6.2"
